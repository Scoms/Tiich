<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/Bootstrap/css/bootstrap.min.css")
        
    @Styles.Render("~/Content/_Layout.css")
    @Styles.Render("~/Content/Gmap.css")
    
    
</head>
<body>
    @Html.Partial("Navbar")
    @Html.Partial("Errors")
    @Html.Partial("SuccessMessage")
     
    @RenderBody()
    
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
