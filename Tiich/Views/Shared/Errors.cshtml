
@if(TempData["errors"] != null)
{
    if(!String.IsNullOrEmpty(TempData["errors"].ToString()))
    {
        <div>
            <div class="col-lg-offset-1 col-lg-10">

                <div class="alert alert-danger">
                    @TempData["errors"]
                </div>
            </div>
        </div>
    }   
}
    