@model List<TiichDAL.Workshop>

<div class="col-lg-6">
    <h1>Top 5</h1>
    <ul class="list-group">
        @for (int i = 0; i < 5; i++)
        {
            <li class="list-group-item">
                @Model.ElementAt(i).Label
            </li>
        }
    </ul>

</div>
<div class="col-lg-6">
    <h1>Flop 5</h1>
    <ul class="list-group">
        @for (int i = 9; i >= 5; i--)
        {
            <li class="list-group-item">
                @Model.ElementAt(i).Label
            </li>
        }
    </ul>
</div>